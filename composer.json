{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "aliqasemzadeh/livewire-bootstrap-modal": "^2.1",
        "arcanedev/log-viewer": "^10.1",
        "barryvdh/laravel-dompdf": "^2.0",
        "doctrine/dbal": "^3.0",
        "guzzlehttp/guzzle": "^7.8",
        "infinityxtech/filament-unlayer": "^1.0",
        "jantinnerezo/livewire-alert": "^4.0",
        "kodeine/laravel-meta": "^2.2",
        "laravel-notification-channels/fcm": "^4.3",
        "laravel/framework": "^10.0",
        "laravel/jetstream": "^4.3",
        "laravel/octane": "^2.8",
        "laravel/pennant": "^1.7",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.7",
        "livewire-ui/modal": "^2.0",
        "livewire/livewire": "^3.0",
        "maatwebsite/excel": "^3.1",
        "midtrans/midtrans-php": "^2.6",
        "ralphjsmit/livewire-urls": "^1.4",
        "shetabit/visitor": "^4.3",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-sitemap": "^7.2",
        "tightenco/ziggy": "^2.3",
        "webklex/laravel-pdfmerger": "^1.3",
        "yajra/laravel-datatables": "10",
        "yajra/laravel-datatables-export": "^10.1",
        "yajra/laravel-datatables-html": "10.0",
        "yajra/laravel-datatables-oracle": "^10.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "modules/"
        },
        "files": [
            "app/Helpers/Vacation.php",
            "app/Helpers/Postyear.php",
            "app/Helpers/Taxrate.php",
            "app/Helpers/CMS.php",
            "app/Helpers/Device.php",
            "app/Helpers/Inwords.php",
            "app/Helpers/Setting.php",
            "app/Helpers/BreakJson.php",
            "modules/Core/Helpers/Cutoff.php",
            "app/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "wikimedia/composer-merge-plugin": false
        },
        "platform": {
            "php": "8.2"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
